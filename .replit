modules = ["cpp-clang14", "c-clang14"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "run"

[[workflows.workflow]]
name = "run"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "clang++ -std=c++17 main.cpp -o main && ./main"

[workflows.workflow.metadata]
outputType = "console"
